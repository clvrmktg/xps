-
  function arc(width, height) {
    let aspectRatio = width / 190;
    
    let new_width = parseFloat((height * aspectRatio).toFixed(2))

    return new_width

  }

-
  function get_height(width, new_width) {

    let aspectRatio = width / 190;
    
    let new_height = parseFloat((new_width / aspectRatio).toFixed(2))

    return new_height

  }

